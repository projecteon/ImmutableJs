{
    "env": {
        "development": {
            "presets": ["es2015", "stage-0", "stage-1", "stage-2"]
        },
        "commonjs": {
            "presets": ["es2015", "stage-0", "stage-1", "stage-2"]
        },
        "es6": {
            "presets": ["es2015", "stage-0", "stage-1", "stage-2"]
        },
        "production": {
            "presets": ["es2015", "stage-0", "stage-1", "stage-2"],
            "plugins": ["transform-object-assign"]
        },
        "testing": {
            "presets": ["es2015", "stage-0", "stage-1", "stage-2"],
            "plugins": ["transform-object-assign"]
        }
    }
}
